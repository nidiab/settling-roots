    "preview": "vite preview",
    "lint": "eslint \"**/*.{js,jsx}\"",
    "format": "prettier . --write",
    "format:check": "prettier . --check",
    "prepare": "husky install"
  },
  "dependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.26.2"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^3.1.0",
    "eslint": "^8.57.0",
