    "eslint-plugin-react": "^7.35.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.3.0",
    "prettier": "^3.3.3",
    "vite": "^4.5.3"
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "eslint --fix",
